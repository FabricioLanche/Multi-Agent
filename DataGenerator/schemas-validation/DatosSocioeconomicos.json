{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "DatosSocioeconomicos",
    "type": "object",
    "x-dynamodb": {
        "partition_key": "usuarioId",
        "sort_key": "id"
    },
    "properties": {
        "id": {
            "type": "string",
            "pattern": "^[0-9a-fA-F\\-]{36}$"
        },
        "usuarioId": {
            "type": "string",
            "pattern": "^[0-9a-fA-F\\-]{36}$"
        },
        "tipo_financiamiento": {
            "type": "string",
            "enum": [
                "PROPIO",
                "FAMILIAR",
                "BECA",
                "CREDITO",
                "OTRO"
            ]
        },
        "situacion_laboral": {
            "type": "string",
            "enum": [
                "TRABAJA",
                "NO_TRABAJA",
                "TRABAJA_Y_ESTUDIA"
            ]
        },
        "ingreso_estimado": {
            "type": "number",
            "minimum": 0,
            "description": "Ingreso mensual estimado en moneda local"
        },
        "dependencia_economica": {
            "type": "boolean",
            "description": "Indica si depende econ√≥micamente de otra(s) personas"
        }
    },
    "required": [
        "id",
        "usuarioId",
        "tipo_financiamiento",
        "situacion_laboral"
    ],
    "additionalProperties": false
}